<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen21">

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView21"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text="&lt;"
        android:id="@+id/button40"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="36sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="19/25"
        android:id="@+id/textView22"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pozor! Anketár nikdy neponúkne možnosť neviem!!!"
        android:id="@+id/textView108"
        android:textColor="#f44336"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Ak by sa tento víkend konali voľby na predsedu (župana) Prešovského samosprávneho  kraja (PSK), ktorého kandidáta zo zoznamu by ste volili za predsedu PSK?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView107"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/textView103"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Dajte respondentovi do ruky zoznam kandidátov na predsedu (župana) PSK  a zakrúžkujte len jednu možnosť podľa odpovede respondenta."
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView103"
        android:textColor="#f44336"
        android:textStyle="normal|italic"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/textView22"
        android:layout_alignBottom="@+id/textView22"
        android:layout_toLeftOf="@+id/textView22"
        android:layout_toStartOf="@+id/textView22" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jozef Mihalčin (ĽS Naše Slovensko strana  Mariana Kotlebu"
        android:id="@+id/checkBox67"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox66"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Andrea Turčanová  (KDH)"
        android:id="@+id/checkBox72"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox67"
        android:layout_alignLeft="@+id/checkBox67"
        android:layout_alignStart="@+id/checkBox67" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peter Pčolinský (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox70"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox72"
        android:layout_alignLeft="@+id/checkBox72"
        android:layout_alignStart="@+id/checkBox72" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peter Chudík  (SMER-SD)"
        android:id="@+id/checkBox69"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox70"
        android:layout_alignLeft="@+id/checkBox70"
        android:layout_alignStart="@+id/checkBox70" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jozef Lukáč (OĽaNO –  NOVA, SaS)"
        android:id="@+id/checkBox66"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/button40"
        android:layout_alignLeft="@+id/checkBox67"
        android:layout_alignStart="@+id/checkBox67" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox74"
        android:checked="false"
        android:textColor="#878787"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox70"
        android:layout_alignRight="@+id/checkBox66"
        android:layout_alignEnd="@+id/checkBox66"
        android:layout_marginRight="33dp"
        android:layout_marginEnd="33dp"
        android:layout_marginTop="69dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox75"
        android:checked="false"
        android:textColor="#878787"
        android:onClick="onCheckboxClicked"
        android:layout_alignBaseline="@+id/checkBox74"
        android:layout_alignBottom="@+id/checkBox74"
        android:layout_alignLeft="@+id/checkBox69"
        android:layout_alignStart="@+id/checkBox69" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text=">"
        android:id="@+id/button41"
        android:textSize="36sp"
        android:layout_below="@+id/textView107"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jaroslav Regec  (SNS)"
        android:id="@+id/checkBox71"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_above="@+id/checkBox66"
        android:layout_alignLeft="@+id/checkBox66"
        android:layout_alignStart="@+id/checkBox66" />
</RelativeLayout>
