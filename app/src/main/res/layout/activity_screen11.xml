<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen11">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Späť"
        android:id="@+id/button20"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ďalej"
        android:id="@+id/button21"
        android:layout_alignTop="@+id/button20"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText13"
        android:text="Dajte respondentovi do ruky zoznam kandidátov na predsedu (župana) BSK  a zakrúžkujte možnosti podľa odpovede respondenta."
        android:textStyle="italic"
        android:textColor="#F44336"
        android:textAlignment="center"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_toLeftOf="@+id/button21"
        android:layout_toStartOf="@+id/button21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView23"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="10dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText13"
        android:layout_alignParentLeft="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText14"
        android:text="Môžete mi povedať, ktorého kandidáta na predsedu (župana) Bratislavského  samosprávneho kraja (BSK) zo zoznamu poznáte?"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textSize="30dp"
        android:layout_below="@+id/textView23"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="64dp"
        android:layout_alignRight="@+id/checkBox51"
        android:layout_alignEnd="@+id/checkBox51" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Juraj Droba (SaS,  OľaNO-NOVA, KDH)"
        android:id="@+id/checkBox48"
        android:checked="false"
        android:layout_marginTop="77dp"
        android:layout_below="@+id/editText14"
        android:layout_toLeftOf="@+id/checkBox49"
        android:layout_toStartOf="@+id/checkBox49" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pavol Frešo  (Nezávislý)"
        android:id="@+id/checkBox49"
        android:checked="false"
        android:layout_above="@+id/checkBox50"
        android:layout_alignLeft="@+id/checkBox51"
        android:layout_alignStart="@+id/checkBox51" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Milan Ftáčnik (SMER-  SD, SNS, MOST-HÍD)"
        android:id="@+id/checkBox50"
        android:checked="false"
        android:layout_below="@+id/checkBox48"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Boris Kollár (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox51"
        android:checked="false"
        android:layout_alignTop="@+id/checkBox50"
        android:layout_toRightOf="@+id/checkBox50"
        android:layout_toEndOf="@+id/checkBox50" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox52"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignTop="@+id/checkBox53"
        android:layout_toRightOf="@+id/imageView11"
        android:layout_toEndOf="@+id/imageView11" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox53"
        android:checked="false"
        android:textColor="#878787"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/checkBox51"
        android:layout_alignRight="@+id/checkBox49"
        android:layout_alignEnd="@+id/checkBox49"
        android:layout_marginRight="41dp"
        android:layout_marginEnd="41dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="10/25"
        android:id="@+id/textView10"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
