<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen23">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pozor! Anketár nikdy neponúkne možnosť neviem!!!"
        android:id="@+id/textView114"
        android:textColor="#f44336"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView23"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pavol  Frešo  (Nezávislý  )"
        android:id="@+id/checkBox162"
        android:checked="false"
        android:layout_below="@+id/checkBox161"
        android:layout_alignLeft="@+id/checkBox161"
        android:layout_alignStart="@+id/checkBox161"
        android:textSize="18sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Milan  Ftáčnik (SMER-  SD, SNS,  MOST-HÍD)"
        android:id="@+id/checkBox163"
        android:checked="false"
        android:layout_below="@+id/checkBox162"
        android:layout_alignLeft="@+id/checkBox162"
        android:layout_alignStart="@+id/checkBox162"
        android:textSize="18sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Boris Kollár (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox164"
        android:checked="false"
        android:layout_below="@+id/checkBox163"
        android:layout_toRightOf="@+id/button44"
        android:layout_toEndOf="@+id/button44"
        android:textSize="18sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nešiel by som voliť"
        android:id="@+id/checkBox165"
        android:checked="false"
        android:layout_marginTop="20dp"
        android:textColor="#878787"
        android:layout_below="@+id/checkBox164"
        android:layout_toRightOf="@+id/button44"
        android:layout_toEndOf="@+id/button44" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="21/25"
        android:id="@+id/textView25"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text="&lt;"
        android:id="@+id/button44"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="36sp" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text=">"
        android:id="@+id/button45"
        android:layout_alignBaseline="@+id/button44"
        android:layout_alignBottom="@+id/button44"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="36sp" />

    <TextView
        android:text="Otázka: Ak by sa tento víkend konali voľby na predsedu (župana) Bratislavského  samosprávneho kraja (BSK), ktorého kandidáta zo zoznamu by ste volili za  predsedu BSK?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView113"
        android:textStyle="normal|bold"
        android:textSize="30sp"
        android:textColor="#000000"
        android:layout_marginTop="61dp"
        android:layout_below="@+id/textView110"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Juraj Droba (SaS, OľaNO-NOVA,  KDH)"
        android:id="@+id/checkBox161"
        android:checked="false"
        android:layout_marginTop="60dp"
        android:layout_below="@+id/button44"
        android:layout_alignLeft="@+id/checkBox164"
        android:layout_alignStart="@+id/checkBox164"
        android:textSize="18sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox166"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignBaseline="@+id/checkBox167"
        android:layout_alignBottom="@+id/checkBox167"
        android:layout_toRightOf="@+id/checkBox167"
        android:layout_toEndOf="@+id/checkBox167"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox167"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignBaseline="@+id/checkBox165"
        android:layout_alignBottom="@+id/checkBox165"
        android:layout_alignRight="@+id/checkBox161"
        android:layout_alignEnd="@+id/checkBox161"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp" />

    <TextView
        android:text="Pokyn: Dajte respondentovi do ruky zoznam kandidátov na predsedu (župana)  BSK a zakrúžkujte len jednu možnosť podľa odpovede respondenta."
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView110"
        android:textStyle="normal|italic"
        android:textSize="18sp"
        android:textColor="#f44336"
        android:layout_alignBaseline="@+id/textView25"
        android:layout_alignBottom="@+id/textView25"
        android:layout_toLeftOf="@+id/button45"
        android:layout_toStartOf="@+id/button45" />
</RelativeLayout>
