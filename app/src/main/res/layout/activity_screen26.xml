<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen26">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Späť"
        android:id="@+id/button50"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ďalej"
        android:id="@+id/button51"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView26"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText62"
        android:text="Dajte respondentovi do ruky zoznam kandidátov na predsedu (župana)  BSK a zakrúžkujte len jednu možnosť podľa odpovede respondenta."
        android:textStyle="italic"
        android:textColor="#F44336"
        android:textAlignment="center"
        android:layout_below="@+id/imageView26"
        android:layout_alignRight="@+id/editText63"
        android:layout_alignEnd="@+id/editText63"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText63"
        android:textStyle="bold"
        android:textSize="30dp"
        android:text="Ak by Váš kandidát na predsedu (župana) Bratislavského samosprávneho  kraja (BSK) odstúpil, koho ďalšieho zo zoznamu by ste volili za predsedu  BSK?"
        android:textAlignment="center"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/button51"
        android:layout_alignEnd="@+id/button51"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Juraj Droba (SaS, OľaNO-NOVA,  KDH)"
        android:id="@+id/checkBox173"
        android:layout_marginTop="39dp"
        android:checked="false"
        android:layout_below="@+id/editText63"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pavol  Frešo  (Nezávislý  )"
        android:id="@+id/checkBox174"
        android:checked="false"
        android:layout_below="@+id/checkBox173"
        android:layout_alignLeft="@+id/checkBox173"
        android:layout_alignStart="@+id/checkBox173" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Milan  Ftáčnik (SMER-  SD, SNS,  MOST-HÍD)"
        android:id="@+id/checkBox175"
        android:checked="false"
        android:layout_below="@+id/checkBox174"
        android:layout_alignLeft="@+id/checkBox174"
        android:layout_alignStart="@+id/checkBox174" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Boris Kollár (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox176"
        android:checked="false"
        android:layout_below="@+id/checkBox175"
        android:layout_alignLeft="@+id/checkBox175"
        android:layout_alignStart="@+id/checkBox175" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nešiel by som voliť"
        android:id="@+id/checkBox177"
        android:checked="false"
        android:textColor="#878787"
        android:layout_below="@+id/checkBox176"
        android:layout_alignLeft="@+id/checkBox176"
        android:layout_alignStart="@+id/checkBox176"
        android:layout_marginTop="25dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox178"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignTop="@+id/checkBox177"
        android:layout_toRightOf="@+id/checkBox177"
        android:layout_toEndOf="@+id/checkBox177" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox179"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignTop="@+id/checkBox178"
        android:layout_toRightOf="@+id/checkBox178"
        android:layout_toEndOf="@+id/checkBox178" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="24/25"
        android:id="@+id/textView29"
        android:layout_alignBottom="@+id/button51"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
