<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen26">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pozor! Anketár nikdy neponúkne možnosť neviem!!!"
        android:id="@+id/textView123"
        android:textColor="#f44336"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView2"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pavol  Frešo  (Nezávislý  )"
        android:id="@+id/checkBox174"
        android:checked="false"
        android:layout_below="@+id/checkBox173"
        android:layout_alignLeft="@+id/checkBox173"
        android:layout_alignStart="@+id/checkBox173"
        android:textSize="18sp"
        android:onClick="onCheckboxClicked" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Milan  Ftáčnik (SMER-  SD, SNS,  MOST-HÍD)"
        android:id="@+id/checkBox175"
        android:checked="false"
        android:layout_below="@+id/checkBox174"
        android:layout_alignLeft="@+id/checkBox174"
        android:layout_alignStart="@+id/checkBox174"
        android:textSize="18sp"
        android:onClick="onCheckboxClicked" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Boris Kollár (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox176"
        android:checked="false"
        android:layout_below="@+id/checkBox175"
        android:layout_alignLeft="@+id/checkBox175"
        android:layout_alignStart="@+id/checkBox175"
        android:textSize="18sp"
        android:onClick="onCheckboxClicked" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nešiel by som voliť"
        android:id="@+id/checkBox177"
        android:checked="false"
        android:textColor="#878787"
        android:layout_below="@+id/checkBox176"
        android:layout_alignLeft="@+id/checkBox176"
        android:layout_alignStart="@+id/checkBox176"
        android:layout_marginTop="25dp"
        android:onClick="onCheckboxClicked" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox178"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignTop="@+id/checkBox177"
        android:layout_toRightOf="@+id/checkBox177"
        android:layout_toEndOf="@+id/checkBox177"
        android:onClick="onCheckboxClicked" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox179"
        android:checked="false"
        android:textColor="#878787"
        android:layout_alignTop="@+id/checkBox178"
        android:layout_toRightOf="@+id/checkBox178"
        android:layout_toEndOf="@+id/checkBox178"
        android:onClick="onCheckboxClicked" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="24/25"
        android:id="@+id/textView29"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Otázka: Ak by Váš kandidát na predsedu (župana) Bratislavského samosprávneho  kraja (BSK) odstúpil, koho ďalšieho zo zoznamu by ste volili za predsedu  BSK?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView122"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="125dp"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pokyn: Prečítajte otázku aj predtlačené možnosti a zakrúžkujte len jednu možnosť podľa  odpovede respondenta."
        android:id="@+id/textView121"
        android:textColor="#f44336"
        android:textStyle="italic"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textView29"
        android:layout_toStartOf="@+id/textView29" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text="&lt;"
        android:id="@+id/button50"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="36sp" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text=">"
        android:id="@+id/button51"
        android:layout_alignBaseline="@+id/button50"
        android:layout_alignBottom="@+id/button50"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="36sp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Juraj Droba (SaS, OľaNO-NOVA,  KDH)"
        android:id="@+id/checkBox173"
        android:checked="false"
        android:textSize="18sp"
        android:layout_marginTop="95dp"
        android:layout_below="@+id/button51"
        android:layout_centerHorizontal="true"
        android:onClick="onCheckboxClicked" />

</RelativeLayout>
