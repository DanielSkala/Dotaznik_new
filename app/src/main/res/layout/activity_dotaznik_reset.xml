<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dotaznik_reset"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.DotaznikReset">

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView127"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="141dp"
        android:id="@+id/button57" />

    <TextView
        android:text="Naozaj si prajete resetovať dotazník?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="274dp"
        android:id="@+id/textView127"
        android:textSize="36sp"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:textAlignment="center" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView127"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="141dp"
        android:id="@+id/button55" />

    <Button
        android:text="Áno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button56"
        android:layout_alignBaseline="@+id/button57"
        android:layout_alignBottom="@+id/button57"
        android:layout_alignLeft="@+id/button57"
        android:layout_alignStart="@+id/button57" />

    <Button
        android:text="Nie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="60dp"
        android:layout_marginEnd="60dp"
        android:id="@+id/button58"
        android:layout_alignBaseline="@+id/button57"
        android:layout_alignBottom="@+id/button57"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
