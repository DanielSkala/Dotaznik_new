        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox68"
        android:layout_alignLeft="@+id/checkBox77"
        android:layout_alignStart="@+id/checkBox77" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peter Pčolinský (SME  RODINA – Boris Kollár)"
        android:id="@+id/checkBox77"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox78"
        android:layout_alignLeft="@+id/checkBox79"
        android:layout_alignStart="@+id/checkBox79" />

    <CheckBox
