        android:layout_below="@+id/checkBox77"
        android:layout_toRightOf="@+id/button48"
        android:layout_toEndOf="@+id/button48" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jaroslav Regec  (SNS)"
        android:id="@+id/checkBox73"
        android:checked="false"
        android:textSize="16sp"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox79"
        android:layout_alignLeft="@+id/checkBox79"
        android:layout_alignStart="@+id/checkBox79" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neviem"
        android:id="@+id/checkBox81"
        android:checked="false"
        android:textColor="#878787"
        android:onClick="onCheckboxClicked"
        android:layout_below="@+id/checkBox73"
        android:layout_alignLeft="@+id/checkBox73"
        android:layout_alignStart="@+id/checkBox73"
        android:layout_marginTop="41dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nikoho"
        android:id="@+id/checkBox80"
        android:checked="false"
        android:textColor="#878787"
        android:onClick="onCheckboxClicked"
        android:layout_alignBaseline="@+id/checkBox81"
        android:layout_alignBottom="@+id/checkBox81"
        android:layout_toRightOf="@+id/checkBox73"
        android:layout_toEndOf="@+id/checkBox73" />
</RelativeLayout>
