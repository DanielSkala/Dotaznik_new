<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen25">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pozor! Anketár nikdy neponúkne možnosť neviem!!!"
        android:id="@+id/textView120"
        android:textColor="#f44336"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView25"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="23/25"
        android:id="@+id/textView28"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text="&lt;"
        android:id="@+id/button48"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="36sp" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text=">"
        android:id="@+id/button49"
        android:layout_alignBaseline="@+id/button48"
        android:layout_alignBottom="@+id/button48"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="36sp" />

    <TextView
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Dajte respondentovi do ruky zoznam kandidátov na predsedu (župana) PSK  a zakrúžkujte len jednu možnosť podľa odpovede respondenta."
        android:id="@+id/textView118"
        android:textColor="#f44336"
        android:textStyle="italic"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView25"
        android:layout_toEndOf="@+id/imageView25" />

    <TextView
        android:text="Ak by sa tento víkend konali voľby na predsedu (župana) Prešovského samosprávneho  kraja (PSK), ktorého kandidáta zo zoznamu by ste URČITE NEVOLILI za predsedu PSK?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView119"
        android:textStyle="normal|bold"
        android:textSize="30sp"
        android:textColor="#000000"
        android:layout_below="@+id/textView118"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Andrea Turčanová  (KDH)"
        android:id="@+id/checkBox76"
        android:checked="false"
        android:textSize="16sp"
