<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.daniel.dotaznik.Screen16">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Pozor! Anketár nikdy neponúkne možnosť neviem!!!"
        android:id="@+id/textView92"
        android:textColor="#f44336"
        android:textStyle="bold"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView16"
        android:src="@drawable/obr"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text=">"
        android:id="@+id/button31"
        android:layout_alignTop="@+id/button30"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="36sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="15/25"
        android:id="@+id/textView15"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:text="&lt;"
        android:id="@+id/button30"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="36sp" />

    <TextView
        android:text="@string/i15"
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView90"
        android:textColor="#f44336"
        android:textSize="18sp"
        android:textStyle="normal|italic"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView16"
        android:layout_toEndOf="@+id/imageView16" />

    <TextView
        android:text="@string/q15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView91"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:layout_below="@+id/textView90"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <CheckBox
        android:text="Spokojný"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button30"
        android:layout_toRightOf="@+id/button30"
        android:layout_toEndOf="@+id/button30"
        android:layout_marginLeft="72dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="48dp"
        android:id="@+id/checkBox54"
        android:textSize="18sp" />

    <CheckBox
        android:text="Skôr nespokojný"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox55"
        android:layout_below="@+id/checkBox54"
        android:layout_alignLeft="@+id/checkBox54"
        android:layout_alignStart="@+id/checkBox54"
        android:layout_marginTop="20dp"
        android:textSize="18sp" />

    <CheckBox
        android:text="Neviem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox57"
        android:layout_below="@+id/checkBox55"
        android:layout_toLeftOf="@+id/checkBox56"
        android:layout_toStartOf="@+id/checkBox56"
        android:layout_marginTop="38dp"
        android:textSize="18sp"
        android:textColor="#878787" />

    <CheckBox
        android:text="Skôr spokojný"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox45"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/checkBox54"
        android:layout_alignBottom="@+id/checkBox54"
        android:layout_toRightOf="@+id/checkBox55"
        android:layout_toEndOf="@+id/checkBox55"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp" />

    <CheckBox
        android:text="Nespokojný"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox56"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/checkBox55"
        android:layout_alignBottom="@+id/checkBox55"
        android:layout_alignLeft="@+id/checkBox45"
        android:layout_alignStart="@+id/checkBox45" />

</RelativeLayout>
